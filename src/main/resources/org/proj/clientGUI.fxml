                    </children>
                </VBox>
            </bottom>
            <right>
                <VBox fx:id="VBoxRight" alignment="TOP_CENTER" prefHeight="256.0" prefWidth="183.0" spacing="20.0" BorderPane.alignment="CENTER">
                    <children>
                        <ComboBox fx:id="formatComboBox" prefWidth="150.0" promptText="format">
